show databases;
use pydata;
create table employee(id int, name varchar(32), email varchar(32), salary int);

insert into employee(id, name, email, salary) values(101, "Rahul", "rahul@gmail.com", 45000),(102, "Sonia", "sonia@gmail.com", 55000),(103, "Priya", "priyal@gmail.com", 65000);

select * from employee